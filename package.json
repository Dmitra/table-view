{
  "name": "table-view",
  "version": "0.0.1",
  "description": "Table view for import.io API",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/Dmitra/table-view.git"
  },
  "keywords": [
    "data visualisation",
    "table view",
    "Import.io"
  ],
  "author": "Dmitra",
  "license": "MIT",
  "scripts": {
    "start": "static",
    "build-css": "node-sass scss/index.scss bundle.css",
    "build-min": "browserify src/index.js | uglifyjs -c > bundle.js",
    "build-lib": "browserify dependencies.js | uglifyjs -c > lib.js",
    "watch-js": "watchify src/index.js -d -o bundle.js -v",
    "watch-css": "nodemon -e scss -x \"bash -c 'npm run build-css'\""
  },
  "dependencies": {
    "jquery": "latest",
    "lodash": "latest",
    "handsontable": "https://github.com/handsontable/handsontable",
    "currency-symbol.js": "0.1"
  },
  "devDependencies": {
    "node-static": "latest",
    "node-sass": "latest",
    "browserify": "<=8.1.3",
    "watchify": "latest",
    "nodemon": "latest"
  }
}
